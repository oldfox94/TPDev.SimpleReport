<html lang="de">
	<head>
		<meta charset="UTF-8">		
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">

		<title>SampleAngularJsReport</title>
		
		<!-- Custom tpDev CSS -->
		<link rel="stylesheet" href="styles\custom.css">
		<link rel="stylesheet" href="styles\angularjs_custom.css">
		
		<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js"></script>
		<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-animate.js"></script>

	</head>
	<body ng-app="myApp">
		<div>
			<h1>Sample AngularJs Report</h1>
			<p class="alert alert-info">This ist a sample report from TPDev.SimpleReport using <mark>AngularJs</mark></p>
		</div>
		
		</br>
		
		<p>Collapse / Hide Table <input type="checkbox" ng-model="myCheck"></p>
		</br>
	
		<div id="tblDiv" class="hideDiv" ng-hide="myCheck">
			<table id="TestTbl" class="table table-striped table-bordered" cellspacing="0" width="100%">
			</table>
		</div>
		
		<script>
			var app = angular.module('myApp', ['ngAnimate']);
		</script>
		
